<!--*************录入产品信息*********************************-->
<!--标题-->
<p id="inputProduct_title">——<img src="../images/inputProduct.png" alt="">产品信息管理 ——</p>
<div id="inputProduct_container" class="container">
    <!--录入产品信息的表单-->
    <form action="../php/InsertProductClient.php" method="post"
          enctype="multipart/form-data">
        <!--8个产品信息栏-->
        <div class="in-pd-info">
            <div class="in-pd">
                <span>编号</span>
                <input id="in-pd-id" type="text" name="in-pd-id" required>
            </div>
            <div class="in-pd in-pd-pic">
                <span>图片</span>
                <div>上传图片</div>
                <input type="file" name="in-pd-pic" required>
            </div>
            <div class="in-pd">
                <span>描述</span>
                <input type="text" name="in-pd-description" required>
            </div>
            <div class="in-pd">
                <span>销量</span>
                <input type="text" name="in-pd-saleaccount" required>
            </div>
            <div class="in-pd">
                <span>售价</span>
                <input type="text" name="in-pd-sellprice" required>
            </div>
            <div class="in-pd">
                <span>成本</span>
                <input type="text" name="in-pd-costprice" required>
            </div>
            <div class="in-pd">
                <span>库存</span>
                <input type="text" name="in-pd-remainaccount" required>
            </div>
            <div class="in-pd">
                <span>收益</span>
                <input type="text" name="in-pd-earing" required>
            </div>
        </div>
        <!--2个功能按钮-->
        <div class="in-pd-btns">
            <div class="in-pd-btn1">
                <input type="submit" value="录入产品">
            </div>
            <div class="in-pd-btn2">
                <input id="deleteProduct" value="删除产品">
            </div>
        </div>
    </form>
</div>

