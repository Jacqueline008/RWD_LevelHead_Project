#bg_test{
    position: relative;
    top:250px;
    background-color: purple;
}
body{
    margin:0;
    background-image:url(../images/bg.jpg);
}
#footer{

}

/**********************12个商品容器************************/
/*总的容器*/
#products_container{
    background-color: #fff;
    position: relative;
    top:180px;
    padding: 0;
}
/*行容器*/
#products_container .row{
    margin: 0;
}
/*列容器*/
#products_container .row .pd{
    height:450px;
    padding: 0;
}
/*列中的商品容器*/
.pd-inner{
    width:240px;
    height: 430px;
    border: 1px solid #fff;
    margin: 0 auto;
    margin-top: 10px;
}
.pd-inner:hover{
    border:1px solid #1b6d85;
    box-shadow: 10px 10px 10px #ededed;
    border-radius: 5px;
}
/*商品容器中的图片*/
.pd-inner .pd-pic{
    width:220px;
    height:220px;
    margin: 0 auto;
    margin-top: 0px;
}
.pd-inner .pd-pic a img{
    width:220px;
    height: 220px;
}
/*商品容器中的价格*/
.pd-inner .pd-sellprice{
    width:220px;
    height: 28px;
    background-color: white;
    margin: 0 auto;
    color: #e4393c;
    font-weight: 400;
    font-family: Verdana;
    font-size: 20px;
    margin-top: 0px;
}
.pd-inner .pd-sellprice span{
    width:100px;
}
/*商品容器中的描述*/
.pd-inner .pd-des{
    width:220px;
    height: 70px;
    background-color: white;
    margin: 0 auto;
    margin-top: 0px;
    font-size:14px;
    font-family: "微软雅黑";
}
/*商品容器中的月销量*/
.pd-inner .pd-saleaccount{
    width:220px;
    height: 20px;
    background-color: white;
    margin: 0 auto;
    margin-top: 0px;
    font-family: "微软雅黑";
    font-size:14px;
}
.pd-inner .pd-saleaccount .pd-des-i{
    color: #ffaa71;
}
/*商品容器中的id,成本，库存，收益*/
.pd-inner .pd-mix{
    /*background-color: grey;*/
    width:220px;
    height: 80px;
    margin: 0 auto;
    font-family: "微软雅黑";
    font-size:14px;
}
.pd-inner .pd-mix .pd-id-i,.pd-costprice-i,.pd-remain-i,.pd-earing-i{
    color: #ffaa71;
}

/******************上一页下一页按钮********************************/
/*button_container  begin*/
#button_container{
    height: 35px;
    position: relative;
    top:200px;
}
.btn-container-inner{
    width: 50%;
    height: 35px;
    margin:0 auto;
}
/*上一页按钮*/
#toLast{
    width:70px;
    height:35px;
    border:none;
    font-family:'微软雅黑';
    font-size:16px;
    color:#666;
    border-radius:5px;
    background-color:#ededed;
    transition:background-color 0.4s ease;
    outline:none;
    float: left;
}
#toLast:hover{
    background-color:#FDFDFD;
}
/*下一页按钮*/
#toNext{
    width:70px;
    height:35px;
    border:none;
    font-family:'微软雅黑';
    font-size:16px;
    color:#666;
    border-radius:5px;
    background-color:#ededed;
    transition:background-color 0.4s ease;
    outline:none;
    float: right;
}
#toNext:hover{
    background-color:#FDFDFD;
}
/*button_container  end*/



/*媒体查询*/
