#bg_test{
    position: relative;
    top:200px;
    background-color: purple;
}
body{
    margin:0;
    background-image:url(../images/bg.jpg);
}
#footer{

}
/*******************成功注册容器****************/
/*先隐藏移动端样式*/
#registerSuccess_container_mobile{
    display: none;
}
#registerSuccess_container{
    width:70%;
    height: 450px;
    background-color:#FDFDFD;
    margin: 0px auto;
    position: relative;
    top:120px;
    box-shadow: -10px 10px 10px #CCCCCC;
    border-radius:5px;
}
/*注册成功时 上部分*/
#success_up{
    width:670px;
    height:80px;
    margin: 0 auto;
    padding-top: 70px;
}
/*注册成功时 上部分中图片*/
#success_up #pic{
    float: left;
    width:80px;
    height:80px;
    background-image:url(../images/right.png);
    background-repeat:no-repeat;
    background-size:cover;
    background-position:bottom;
}
/*注册成功时 上部分文字*/
#success_up #text{
    float: left;
    width: 580px;
    height:80px;
}
/*注册成功时 上部分文字段落1*/
#text #pUP{
    font-family: "Microsoft YaHei","Hiragino Sans GB";
    font-size: 30px;
    font-weight: 600;
    line-height: 35px;
    margin: 0;
    padding-top: 5px;
    text-align: center;
}
/*注册成功时 上部分文字段落2*/
#text #pDOWN{
    font-family:'微软雅黑';
    font-size: 18px;
    font-weight: 500;
    line-height: 35px;
    margin: 0;
    padding-top: 5px;
    text-align: center;
}
/*注册成功时 上部分文字段落2里的超链接*/
#text #pDOWN a{
    text-decoration: none;
}
#text #pDOWN a:hover{
    color:#547abb;
}
#text #pDOWN a:visited{
    color:#547abb;
}
#text #pDOWN a:link{
    color:#547abb;
}
/*注册成功下部分*/
#success_down{
    width:600px;
    height:150px;
    margin: 0 auto;
    padding-top: 50px;
}
#success_down #picleft{
    width:270px;
    height:150px;
    float: left;
    background-image:url(../images/registerAfterNewer1.jpg);
    background-repeat:no-repeat;
    background-size:cover;
    background-position:bottom;
}
#success_down #picright{
    width:270px;
    height:150px;
    float: right;
    background-image:url(../images/registerAfterNewer2.jpg);
    background-repeat:no-repeat;
    background-size:cover;
    background-position:bottom;
}


/*媒体查询*/
@media only screen and (max-width: 1200px) {
    #bg_test{
        background-color: gray;
    }
}
@media only screen and (max-width: 992px) {
    #bg_test{
        background-color: pink;
    }
    /*******************成功注册容器****************/
    #registerSuccess_container{
        width: 700px;
    }
}
@media only screen and (max-width: 768px) {
    #bg_test{
        background-color: blue;
    }
    /*******************成功注册容器****************/
    #registerSuccess_container{
        display: none;
    }
    /*成功注册容器移动端样式*/
    #registerSuccess_container_mobile{
        display: block;
        width:95%;
        height: 400px;
        background-color:#FDFDFD ;
        position: relative;
        top:100px;
        margin:0 auto;
    }
    #registerSuccess_container_mobile_inner{
        width:95%;
        height: 380px;
        margin: 0 auto;
        padding-top: 20px;
    }
    #registerSuccess_container_mobile_inner p{
        width: 100%;
        text-align: center;
        font-size: 14px;
    }
    #registerSuccess_container_mobile_inner #picContainer{
        width: 270px;
        margin: 0 auto;
    }

    #footer{
        top:150px;
        padding-bottom: 30px;
    }
}