/***********************猜你喜欢***********************************/
/*标题*/
#yourLikesTitle{
    width:200px;
    margin: 0 auto;
    position: relative;
    top:100px;
    font-family:"微软雅黑";
    color:#555;
    text-align: center;
}
#yourLikesTitle img{
    width:30px;
    height:30px;
    position: relative;
    top:10px;
    margin: 0 5px;
}
/*承装你喜欢商品的容器*/
#yourLikes{
    width:80%;
    height:305px;
    margin: 0 auto;
    position:relative;
    top:150px;
}
/*4个商品*/
.like-pd{
    width:24%;
    float:left;
    background-color: white;
    height: 305px;
}
.like-pd1,.like-pd3{
    margin-right: 1.5%;
}
.like-pd2{
    margin-right: 1%;
}
/*4个商品中的图片*/
.like-pd img{
    width:100%;
    height: 230px;
    margin: 0;
}
/*4个商品中的段落*/
.like-pd p{
    margin: 0;
    width: 95%;
    height: 35px;
    font-size: 14px;
    margin: 0 auto;
}
.like-pd .like-pd-p1{
    line-height: 35px;
    color: #ef7662;
}


/*媒体查询*/
@media only screen and (max-width: 1200px) {
    #bg_test{
        background-color: gray;
    }
    /*****************猜你喜欢*******************************/
    /*4个商品中的图片*/
    .like-pd img{
        width:100%;
        height: 200px;
        margin: 0;
    }
    /*4个商品中的段落*/
    .like-pd p{
        height: 50px;
        font-size: 16px;
    }
    .like-pd .like-pd-p2{
        font-size: 12px;
    }

}
@media only screen and (max-width: 992px) {
    #bg_test{
        background-color: pink;
    }
    /******************************图片形式的导航******************************/
    #menu_pics{
        height:380px;
    }
    #menu_pics a{
        height: 180px;
    }
    /***图片中的黑色字幕***/
    #menu_a_tv #tv_div1,#menu_a_aircondition #aircondition_div1,#menu_a_refrigerator #refrigerator_div1,
    #menu_a_washer #washer_div1,#menu_a_waterheater #waterheater_div1,#menu_a_kitchen #kitchen_div1{
        width:33%;
        font-size:14px;
    }
    /***图片中的透明黑色遮罩***/
    #menu_a_tv #tv_div2,#menu_a_aircondition #aircondition_div2,#menu_a_refrigerator #refrigerator_div2,
    #menu_a_washer #washer_div2,#menu_a_waterheater #waterheater_div2,#menu_a_kitchen #kitchen_div2{
        font-size:16px;
    }

    /*****************猜你喜欢*******************************/
    /*承装你喜欢商品的容器*/
    #yourLikes{
        width:80%;
        height:265px;
        margin: 0 auto;
        position:relative;
        top:150px;
    }
    /*4个商品*/
    .like-pd{
        height: 265px;
    }
    /*4个商品中的图片*/
    .like-pd img{
        width:100%;
        height: 160px;
        margin: 0;
    }

}
@media only screen and (max-width: 768px) {
    #bg_test{
        background-color: blue;
    }
    /*****************************标头下方的网站介绍图*********************************************/
    /*隐藏超小设备以上的效果*/
    #banner{
        display: none;
    }
    /*超小设备效果*/
    #banner_mobile{
        display: block;
        width:100%;
        height:250px;
        background:url(../images/home.jpg);
        background-repeat:no-repeat;
        background-size:cover;
        background-position:bottom;
        position: relative;
        top:65px;
    }
    #banner_mobile span{
        display: block;
        color:#fdfdfd;
        letter-spacing:5px;
        font-family:'微软雅黑';
        font-size:17px;
        font-weight:500;
        line-height:50px;
        width:80%;
        margin: 0 auto;
        padding: 0 auto;
        text-align: center;
        position: relative;
        top:60px;
    }

    /******************************图片形式的导航******************************/
    #menu_pics{
        width:100%;
        height:250px;
        top:100px;
    }
    #menu_pics a{
        height: 120px;
    }
    /***图片中的黑色字幕***/
    #menu_a_tv #tv_div1,#menu_a_aircondition #aircondition_div1,#menu_a_refrigerator #refrigerator_div1,
    #menu_a_washer #washer_div1,#menu_a_waterheater #waterheater_div1,#menu_a_kitchen #kitchen_div1{
        width:45%;
        height:17%;
        font-size:12px;
    }
    /***图片中的透明黑色遮罩***/
    #menu_a_tv #tv_div2,#menu_a_aircondition #aircondition_div2,#menu_a_refrigerator #refrigerator_div2,
    #menu_a_washer #washer_div2,#menu_a_waterheater #waterheater_div2,#menu_a_kitchen #kitchen_div2{
        font-size:14px;
    }

    /*****************猜你喜欢*******************************/
    /*标题*/
    #yourLikesTitle{
        position: relative;
        top:120px;
        font-size: 14px;
    }
    #yourLikesTitle img{
        width:20px;
        height:20px;
        top:5px;
        margin: 0 5px;
    }
    /*承装你喜欢商品的容器*/
    #yourLikes{
        width:100%;
        height: 125px;
    }
    /*4个商品*/
    .like-pd{
        height: 125px;
    }
    /*4个商品中的图片*/
    .like-pd img{
        width:100%;
        height: 90px;
        margin: 0;
    }
    .like-pd .like-pd-p1{
        height: 30px;
        font-size: 14px;
    }
    .like-pd .like-pd-p2{
        display: none;
    }
}


