#bg_test{
    position: relative;
    top:250px;
    background-color: purple;
}
body{
    margin:0;
    background-image:url(../images/bg.jpg);
}
#footer{
    top:250px;
}
/******************录入产品信息***********************************/
/*录入产品信息容器 begin*/
#inputProduct_container{
    height: 150px;
    background-color:#FDFDFD;
    margin:0 auto;
    border-radius:5px;
    position: relative;
    top:100px;
    padding: 0;
}
/*8个产品信息的容器 begin*/
#inputProduct_container .in-pd-info{
    height: 100px;
}
/*承装8个产品信息的每一个小容器*/
.in-pd{
    height: 50px;
    float:left;
    width:25%;
    /*border: 1px solid black;*/
    text-align: center;
    line-height: 50px;
}
/*8个产品信息中的input*/
.in-pd input{
    width:150px;
    height:26px;
    border-radius:5px;
    border:1px grey solid;
    font-size:16px;
    text-align: center;
}
/*8个产品信息中的图片信息一栏*/
.in-pd-pic span{
    position: relative;
    left:-80px;
}
.in-pd-pic input{
    position: relative;
    top:-65px;
    left:85px;
    opacity: 0;
}
.in-pd-pic div{
    width:150px;
    height:26px;
    border-radius:5px;
    position: relative;
    top:-40px;
    left:85px;
    background-color:#ededed;
    line-height: 26px;
}
/*8个产品信息的容器 end*/

/*2个功能按钮(提交+重置)  begin*/
.in-pd-btns{
    height: 50px;
}
/*承装每个按钮的小容器*/
.in-pd-btns>div{
    width:25%;
    height: 50px;
    float:right;
    /*border: 1px solid black;*/
    text-align: center;
    line-height: 50px;
}
.in-pd-btns>div input{
    width:180px;
    height:26px;
    border-radius:5px;
    border:0px grey solid;
    font-size:16px;
    text-align: center;
    line-height: 26px;
}
.in-pd-btn1 input{
    background-color: #547abb;
}
.in-pd-btn2 input{
    background-color: #ef7662;
}
/*2个功能按钮(提交+重置)  end*/
/*录入产品信息容器 end*/


/**********************12个商品容器************************/
/*总的容器*/
#products_container{
    background-color: #fff;
    position: relative;
    top:180px;
    padding: 0;
}
/*行容器*/
#products_container .row{
    margin: 0;
}
/*列容器*/
#products_container .row .pd{
    height:450px;
    padding: 0;
}
/*列中的商品容器*/
.pd-inner{
    width:240px;
    height: 430px;
    border: 1px solid #fff;
    margin: 0 auto;
    margin-top: 10px;
}
.pd-inner:hover{
    border:1px solid #1b6d85;
    box-shadow: 10px 10px 10px #ededed;
    border-radius: 5px;
}
/*商品容器中的图片*/
.pd-inner .pd-pic{
    width:220px;
    height:220px;
    margin: 0 auto;
    margin-top: 0px;
}
.pd-inner .pd-pic a img{
    width:220px;
    height: 220px;
}
/*商品容器中的价格*/
.pd-inner .pd-sellprice{
    width:220px;
    height: 28px;
    background-color: white;
    margin: 0 auto;
    color: #e4393c;
    font-weight: 400;
    font-family: Verdana;
    font-size: 20px;
    margin-top: 0px;
}
.pd-inner .pd-sellprice span{
    width:100px;
}
/*商品容器中的描述*/
.pd-inner .pd-des{
    width:220px;
    height: 70px;
    background-color: white;
    margin: 0 auto;
    margin-top: 0px;
    font-size:14px;
    font-family: "微软雅黑";
}
/*商品容器中的月销量*/
.pd-inner .pd-saleaccount{
    width:220px;
    height: 20px;
    background-color: white;
    margin: 0 auto;
    margin-top: 0px;
    font-family: "微软雅黑";
    font-size:14px;
}
.pd-inner .pd-saleaccount .pd-des-i{
    color: #ffaa71;
}
/*商品容器中的id,成本，库存，收益*/
.pd-inner .pd-mix{
    /*background-color: grey;*/
    width:220px;
    height: 80px;
    margin: 0 auto;
    font-family: "微软雅黑";
    font-size:14px;
}
.pd-inner .pd-mix .pd-id-i,.pd-costprice-i,.pd-remain-i,.pd-earing-i{
    color: #ffaa71;
}

/******************上一页下一页按钮********************************/
/*button_container  begin*/
#button_container{
    height: 35px;
    position: relative;
    top:200px;
}
.btn-container-inner{
    width: 50%;
    height: 35px;
    margin:0 auto;
}
/*上一页按钮*/
#toLast{
    width:70px;
    height:35px;
    border:none;
    font-family:'微软雅黑';
    font-size:16px;
    color:#666;
    border-radius:5px;
    background-color:#ededed;
    transition:background-color 0.4s ease;
    outline:none;
    float: left;
}
#toLast:hover{
    background-color:#FDFDFD;
}
/*下一页按钮*/
#toNext{
    width:70px;
    height:35px;
    border:none;
    font-family:'微软雅黑';
    font-size:16px;
    color:#666;
    border-radius:5px;
    background-color:#ededed;
    transition:background-color 0.4s ease;
    outline:none;
    float: right;
}
#toNext:hover{
    background-color:#FDFDFD;
}
/*button_container  end*/



